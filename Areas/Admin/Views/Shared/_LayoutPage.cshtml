<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Admin Panel Justice Inn</title>
    <meta name="description" content="Justice Inn - Legal Website" />
    <meta name="keywords" content="Justice Inn, Consertium, Lawyears, Legal, online Nikkah, Child Custoday, Corporate Lawyears, Criminal Matters, Online Divorce, NAB, Consumer Court, Medical Negligence, FIR & Police Assistance, Labour Rights, Inheritence & Succession Certificate" />
    <meta name="author" content="" />
    <link href="~/content/assets/images/logo.png" rel="icon">
    <!-- Vendor Css-->

    @Styles.Render("~/Content/Devcss3")
    @Styles.Render("~/Content/Devcss")

    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.7.2/css/all.css" integrity="sha384-fnmOCqbTlWIlj8LyTjo7mOUStjsKC4pOpQbqyi7RrhN7udi9RwhKkMHpvLbHG9Sr" crossorigin="anonymous">
    <link rel="stylesheet" href="~/Content/assets/css/bootstrap.css" />
    <link rel="stylesheet" href="~/Content/assets/css/LineIcons.min.css" />
    <link rel="stylesheet" href="~/Content/assets/css/swiper.css" />
    <link rel="stylesheet" type="text/css" href="~/Content/OwlCarousel/dist/assets/owl.carousel.min.css">
    <link rel="stylesheet" type="text/css" href="~/Content/OwlCarousel/dist/assets/owl.theme.default.min.css">
    <link href="~/Content/assets/css/style-dark.css" rel="stylesheet">
    <link href="~/Content/assets/css/custom.css" rel="stylesheet">
</head>
<body>
    <div id="preloader" class="preloader">
        <div class="spinner-grow text-dark" role="status">
            <span class="sr-only">Loading...</span>
        </div>
    </div>

    <aside class="aside">
        <div class="nav-wrapper">
            <div class="navbar-toggler">
                <svg class="ham hamRotate ham1" viewBox="0 0 100 100" width="40">
                    <path class="line top" d="m 30,33 h 40 c 0,0 9.044436,-0.654587 9.044436,-8.508902 0,-7.854315 -8.024349,-11.958003 -14.89975,-10.85914 -6.875401,1.098863 -13.637059,4.171617 -13.637059,16.368042 v 40" />
                    <path class="line middle" d="m 30,50 h 40" />
                    <path class="line bottom" d="m 30,67 h 40 c 12.796276,0 15.357889,-11.717785 15.357889,-26.851538 0,-15.133752 -4.786586,-27.274118 -16.667516,-27.274118 -11.88093,0 -18.499247,6.994427 -18.435284,17.125656 l 0.252538,40" />
                </svg>
            </div>
            <nav class="navbar">
                <div class="collapse navbar-collapse show" id="navbarCollapse">
                    <div class="about-avatar mb-3 drop">
                        <a href='@Url.Action("Index","Home")'>
                            <img src="~/content/assets/images/logo.png" alt="" class="img-fluid mx-auto d-block shadow-sm rounded-full">
                        </a>
                    </div>
                    <ul class="navbar-nav mx-auto">
                        <li class="nav-item active">
                            <a class="nav-link text-underline" href='@Url.Action("Index","Admin")'><i class="lni lni-star-filled"></i>Admin Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-underline" href='@Url.Action("Blogs","Admin")'><i class="lni lni-star-filled"></i>Blogs</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-underline" href='@Url.Action("News","Admin")'><i class="lni lni-star-filled"></i>News</a>
                        </li>
                    </ul>
                    <div class="nav-number mt-4">
                        <label class="color-primary">Call us at</label>
                        <p>+92-321-4468358 | +92-42-37350687</p>
                    </div>
                </div>
                <div class="aside-footer mt-5">
                    <ul class="list-unstyled list-inline">
                        <li class="list-inline-item mr-2"><a href="#"><i class="lni lni-facebook-filled border-radius-icons"></i></a></li>
                        <li class="list-inline-item mr-2"><a href="#"><i class="lni lni-twitter-filled border-radius-icons"></i></a></li>
                        <li class="list-inline-item"><a href="#"><i class="lni lni-instagram-original border-radius-icons"></i></a></li>
                    </ul>
                    <p class="copyright-text">Copyright 2019, All Right Reserved</p>
                </div>
            </nav>
        </div>

    </aside>
    @RenderBody()


    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/Customjs")
    @Scripts.Render("~/bundles/Devscripts")

    @RenderSection("scripts", required: false)
</body>
</html>
